library(pacman)
pacman::p_load(tidyverse,
ggpubr,
rstanarm)
#Defining variables
b0 <- 10
b1 <- 6.8
e <- rnorm(100, mean = 1, sd = 0.5)
x0 <- rnorm(100, mean = 5, sd = 2)
y0 <- b0 + b1*x0+e
df_2 <- data.frame(x0 = x0, y0 = y0, b0 = b0, b1 = b1, e = e)
fit <- stan_glm(y0 ~ x0, data = df_2, QR = TRUE)
fit <- stan_glm(y0 ~ x0, data = df_2)
fit <- stan_glm(y0 ~ x0, data = df_2, family = gaussian())
fit <- stan_glm(y0 ~ x0, data = df_2)
fit <- stan_glm(y0 ~ x0, data = df_2, chains = 1)
fit <- stan_glm(y0 ~ x0, data = df_2, chains = 1)
prior_summary(fit)
help('prior_summary.stanreg')
fit <- stan_glm(y0 ~ x0, data = df_2, chains = 1)
prior_summary(fit)
help('prior_summary.stanreg')
